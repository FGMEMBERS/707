		<set>
			<property>/b707/hydraulic/hyd-fluid-shutoff-cover[1]</property>
			<value>0</value>
		</set>
		<set>
			<property>/b707/hydraulic/hyd-fluid-shutoff[0]</property>
			<value>0</value>
		</set>
		<set>
			<property>/b707/hydraulic/hyd-fluid-shutoff[1]</property>
			<value>0</value>
		</set>
		<set>
			<property>/b707/hydraulic/hyd-fluid-pump[0]</property>
			<value>0</value>
		</set>
		<set>
			<property>/b707/hydraulic/hyd-fluid-pump[1]</property>
			<value>0</value>
		</set>

	</init>

	<step>
		<message>
			Plug in the External Power Unit first.
			Select in the main menu: Boeing 707/ External Power Unit
		</message>
		<view>
				<heading-offset-deg>240.8</heading-offset-deg>
				<pitch-offset-deg>4.0</pitch-offset-deg>
				<roll-offset-deg>0.0</roll-offset-deg>
				<x-offset-m>-0.4</x-offset-m>
				<y-offset-m>1.2</y-offset-m>
				<z-offset-m>-18.2</z-offset-m>
				<field-of-view>49.1</field-of-view>
		</view>

		<marker>
				<x-m>-17.5636</x-m>
				<y-m>0.5711</y-m>
				<z-m>1.3967</z-m>
				<scale>1.6612</scale>
		</marker>

		<exit>
			<condition>
				<property>/b707/external-power-connected</property>
			</condition>
		</exit>
	</step>
	
	<step>
		<message>
			External Power Unit closed.
		</message>
		<view>
		    <view-number>1</view-number>
				<heading-offset-deg>195.9</heading-offset-deg>
				<pitch-offset-deg>-3.7</pitch-offset-deg>
				<roll-offset-deg>0.0</roll-offset-deg>
				<x-offset-m>25.0</x-offset-m>
				<y-offset-m>2.0</y-offset-m>
				<z-offset-m>-45.0</z-offset-m>
				<field-of-view>21.6</field-of-view>
		</view>
		<exit>
			<condition>
				<property>/b707/external-power-connected</property>
			</condition>
		</exit>
	</step>
	
	<step>
		<message>
			Set battery main switch.
		</message>
		<view>
				<heading-offset-deg>235.4</heading-offset-deg>
				<pitch-offset-deg>7.9</pitch-offset-deg>
				<roll-offset-deg>0.0</roll-offset-deg>
				<x-offset-m>-0.3</x-offset-m>
				<y-offset-m>1.2</y-offset-m>
				<z-offset-m>-18.2</z-offset-m>
				<field-of-view>51.5</field-of-view>
		</view>

		<marker>
				<x-m>-17.5901</x-m>
				<y-m>0.5711</y-m>
				<z-m>1.4587</z-m>
				<scale>2.3719</scale>
		</marker>

		<error>
			<message>Open battery switch cover.</message>
			<condition>
				<not><property>/b707/battery-switch</property></not>
			</condition>
		</error>
		<exit>
			<condition>
				<property>/b707/battery-switch</property>
			</condition>
		</exit>
	</step>
	
	<step>
		<message>
			SELECTOR LOAD &amp; VOLTS set to BUS-TIE.
		</message>
		<view>
				<heading-offset-deg>234.0</heading-offset-deg>
				<pitch-offset-deg>14.4</pitch-offset-deg>
				<roll-offset-deg>0.0</roll-offset-deg>
				<x-offset-m>-0.3</x-offset-m>
				<y-offset-m>1.2</y-offset-m>
				<z-offset-m>-18.2</z-offset-m>
				<field-of-view>14.1</field-of-view>
		</view>

		<marker>
				<x-m>-17.5554</x-m>
				<y-m>0.5711</y-m>
				<z-m>1.4628</z-m>
				<scale>3.0000</scale>
		</marker>
		<error>
			<message>Use setting BUS TIE.</message>
			<condition>
				<or>
					<less-than>
						<property>/b707/load-volt-selector</property>
						<value>1</value>
					</less-than>
					<greater-than>
						<property>/b707/load-volt-selector</property>
						<value>1</value>
					</greater-than>
				</or>
			</condition>
		</error>
		<exit>
			<condition>
				<equals>
					<property>/b707/load-volt-selector</property>
					<value>1</value>
				</equals>
			</condition>
		</exit>
	</step>
	
	<step>
		<message>
			ESSENTIAL POWER set to EXT PWR.
		</message>
		<view>
				<heading-offset-deg>238.1</heading-offset-deg>
				<pitch-offset-deg>7.5</pitch-offset-deg>
				<roll-offset-deg>0.0</roll-offset-deg>
				<x-offset-m>-0.3</x-offset-m>
				<y-offset-m>1.2</y-offset-m>
				<z-offset-m>-18.2</z-offset-m>
				<field-of-view>14.5</field-of-view>
		</view>

		<marker>
				<x-m>-17.5620</x-m>
				<y-m>0.6000</y-m>
				<z-m>1.3686</z-m>
				<scale>1.3967</scale>
		</marker>
		<error>
			<message>turn to EXT PWR.</message>
			<condition>
				<or>
					<less-than>
						<property>/b707/ess-power-switch</property>
						<value>5</value>
					</less-than>
					<greater-than>
						<property>/b707/ess-power-switch</property>
						<value>5</value>
					</greater-than>
				</or>
			</condition>
		</error>
		<exit>
			<condition>
				<equals>
					<property>/b707/ess-power-switch</property>
					<value>5</value>
				</equals>
			</condition>
		</exit>
	</step>
	
	<step>
		<message>
			SELECTOR AC PARALLELING set to External Power.
			The AC source will be synchrosized to 400Hz.
		</message>
		<view>
				<heading-offset-deg>241.4</heading-offset-deg>
				<pitch-offset-deg>3.4</pitch-offset-deg>
				<roll-offset-deg>0.0</roll-offset-deg>
				<x-offset-m>-0.4</x-offset-m>
				<y-offset-m>1.2</y-offset-m>
				<z-offset-m>-18.2</z-offset-m>
				<field-of-view>15.9</field-of-view>
		</view>

		<marker>
				<x-m>-17.6165</x-m>
				<y-m>0.6000</y-m>
				<z-m>1.2488</z-m>
				<scale>1.3967</scale>
		</marker>
		<error>
			<message>select External Power.</message>
			<condition>
				<or>
					<less-than>
						<property>/b707/ac/ac-para-select</property>
						<value>6</value>
					</less-than>
					<greater-than>
						<property>/b707/ac/ac-para-select</property>
						<value>6</value>
					</greater-than>
				</or>
			</condition>
		</error>
		<exit>
			<condition>
				<equals>
					<property>/b707/ac/ac-para-select</property>
					<value>6</value>
				</equals>
			</condition>
		</exit>
	</step>

	<step>
		<message>
			AUXILLIARY PUMPS on.
			Auxilliary system pressure is used for the rudder and as an alternate source for brakes.
		</message>
		<view>
				<heading-offset-deg>230.5</heading-offset-deg>
				<pitch-offset-deg>-17.6</pitch-offset-deg>
				<roll-offset-deg>0.0</roll-offset-deg>
				<x-offset-m>-0.3</x-offset-m>
				<y-offset-m>1.2</y-offset-m>
				<z-offset-m>-18.2</z-offset-m>
				<field-of-view>17.7</field-of-view>
		</view>

		<marker>
				<x-m>-17.4422</x-m>
				<y-m>0.7289</y-m>
				<z-m>0.7942</z-m>
				<scale>1.4959</scale>
		</marker>

		<error>
			<message>Switch on both pumps and wait until 3000 PSI RUD HYD PRESS.</message>
			<condition>
				<or>
					<not><property>/b707/hydraulic/ac-aux-pump[0]</property></not>
					<not><property>/b707/hydraulic/ac-aux-pump[1]</property></not>
				</or>
			</condition>
		</error>
		<exit>
			<condition>
			 <and>
				<property>/b707/hydraulic/ac-aux-pump[0]</property>
				<property>/b707/hydraulic/ac-aux-pump[1]</property>
				<greater-than>
					<property>/b707/hydraulic/rudder</property>
					<value>2900</value>
				</greater-than>
			 </and>
			</condition>
		</exit>
	</step>
	
	<step>
		<message>
			INTERCONNECT VALVE to system.
			AC AUXILLIARY PUMPS supplies also the system pressure.
		</message>
		<view>
				<heading-offset-deg>230.5</heading-offset-deg>
				<pitch-offset-deg>-17.6</pitch-offset-deg>
				<roll-offset-deg>0.0</roll-offset-deg>
				<x-offset-m>-0.3</x-offset-m>
				<y-offset-m>1.2</y-offset-m>
				<z-offset-m>-18.2</z-offset-m>
				<field-of-view>17.7</field-of-view>
		</view>

		<marker>
				<x-m>-17.4066</x-m>
				<y-m>0.7289</y-m>
				<z-m>0.7942</z-m>
				<scale>1.4959</scale>
		</marker>
		
		<error>
			<message>switch knob to system</message>
			<condition>
				<not>
					<equals>
						<property>b707/hydraulic/brake-valve</property>
						<value>2</value>
					</equals>
				</not>
			</condition>
		</error>
		<exit>
			<condition>
				<equals>
					<property>b707/hydraulic/brake-valve</property>
					<value>2</value>
				</equals>
			</condition>
		</exit>
	</step>		
	
	<step>
		<message>
			HYD FLUID SHUTOFFs on.
		</message>
		<view>
				<heading-offset-deg>230.5</heading-offset-deg>
				<pitch-offset-deg>-17.6</pitch-offset-deg>
				<roll-offset-deg>0.0</roll-offset-deg>
				<x-offset-m>-0.3</x-offset-m>
				<y-offset-m>1.2</y-offset-m>
				<z-offset-m>-18.2</z-offset-m>
				<field-of-view>17.7</field-of-view>
		</view>

		<marker>
				<x-m>-17.3207</x-m>
				<y-m>0.7289</y-m>
				<z-m>0.7942</z-m>
				<scale>2.7686</scale>
		</marker>

		<error>
			<message>Open the cover and switch on both shutoff knobs.</message>
			<condition>
				<or>
					<not><property>/b707/hydraulic/hyd-fluid-shutoff[0]</property></not>
					<not><property>/b707/hydraulic/hyd-fluid-shutoff[1]</property></not>
				</or>
			</condition>
		</error>
		<exit>
			<condition>
				<and>
					<property>/b707/hydraulic/hyd-fluid-shutoff[0]</property>
					<property>/b707/hydraulic/hyd-fluid-shutoff[1]</property>
				</and>
			</condition>
		</exit>
	</step>	
	
	<step>
		<message>
			ENGINE PUMPS on.
			The main hydraulic system.
		</message>
		<view>
				<heading-offset-deg>230.5</heading-offset-deg>
				<pitch-offset-deg>-17.6</pitch-offset-deg>
				<roll-offset-deg>0.0</roll-offset-deg>
				<x-offset-m>-0.3</x-offset-m>
				<y-offset-m>1.2</y-offset-m>
				<z-offset-m>-18.2</z-offset-m>
				<field-of-view>17.7</field-of-view>
		</view>

		<marker>
				<x-m>-17.3744</x-m>
				<y-m>0.7289</y-m>
				<z-m>0.7918</z-m>
				<scale>1.5124</scale>
		</marker>

		<error>
			<message>Switch on both pumps and wait until 2200 PSI HYD SYS PRESS.</message>
			<condition>
				<or>
					<not><property>/b707/hydraulic/hyd-fluid-pump[0]</property></not>
					<not><property>/b707/hydraulic/hyd-fluid-pump[1]</property></not>
				</or>
			</condition>
		</error>
		<exit>
			<condition>
				<and>
					<property>/b707/hydraulic/hyd-fluid-pump[0]</property>
					<property>/b707/hydraulic/hyd-fluid-pump[1]</property>
					<greater-than>
						<property>/b707/hydraulic/system</property>
						<value>2000</value>
					</greater-than>
				</and>
			</condition>
		</exit>
	</step>	
		
	<step>
		<message>
			Connect EXTERNAL POWER UNIT and SYNC BUS.
		</message>
		<view>
				<heading-offset-deg>237.8</heading-offset-deg>
				<pitch-offset-deg>8.2</pitch-offset-deg>
				<roll-offset-deg>0.0</roll-offset-deg>
				<x-offset-m>-0.3</x-offset-m>
				<y-offset-m>1.2</y-offset-m>
				<z-offset-m>-18.2</z-offset-m>
				<field-of-view>15.9</field-of-view>
		</view>

		<marker>
				<x-m>-17.5785</x-m>
				<y-m>0.6000</y-m>
				<z-m>1.4091</z-m>
				<scale>1.4959</scale>
		</marker>
		<error>
			<message>
				Set switch to ON.
			</message>
			<condition>
				<not><property>/b707/ground-connect</property></not>
			</condition>
		</error>
		<exit>
			<condition>
				<property>/b707/ground-connect</property>
			</condition>
		</exit>
	</step>

	<step>
		<message>
			Connect the GEN - DRIVES.
			Open the red cover and switch on the drives. 
			Close cover then.
		</message>
		<view>
				<heading-offset-deg>241.8</heading-offset-deg>
				<pitch-offset-deg>14.8</pitch-offset-deg>
				<roll-offset-deg>0.0</roll-offset-deg>
				<x-offset-m>-0.4</x-offset-m>
				<y-offset-m>1.2</y-offset-m>
				<z-offset-m>-18.2</z-offset-m>
				<field-of-view>22.8</field-of-view>
		</view>

		<marker>
				<x-m>-17.8587</x-m>
				<y-m>0.5711</y-m>
				<z-m>1.4603</z-m>
				<scale>1.6612</scale>
		</marker>
		<error>
			<message>
				Connect all GEN - DRIVES and close then all the covers.
				Reconnect in flight is impossible.
