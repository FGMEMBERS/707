			<value>1</value>
		</set>
		<set>
			<property>/controls/engines/engine[3]/cutoff</property>
			<value>1</value>
		</set>
		<set>
			<property>/b707/fuel/valves/valve-pos[0]</property>
			<value>0</value>
		</set>
		<set>
			<property>/b707/fuel/valves/valve-pos[1]</property>
			<value>0</value>
		</set>
		<set>
			<property>/b707/fuel/valves/valve-pos[2]</property>
			<value>0</value>
		</set>
		<set>
			<property>/b707/fuel/valves/valve-pos[3]</property>
			<value>0</value>
		</set>
		<set>
			<property>/b707/fuel/valves/valve-pos[4]</property>
			<value>0</value>
		</set>
		<set>
			<property>/b707/fuel/valves/valve-pos[5]</property>
			<value>0</value>
		</set>
		<set>
			<property>/b707/fuel/valves/boost-pump[0]</property>
			<value>0</value>
		</set>
		<set>
			<property>/b707/fuel/valves/boost-pump[1]</property>
			<value>0</value>
		</set>
		<set>
			<property>/b707/fuel/valves/boost-pump[3]</property>
			<value>0</value>
		</set>
		<set>
			<property>/b707/fuel/valves/boost-pump[4]</property>
			<value>0</value>
		</set>
		<set>
			<property>/b707/fuel/valves/boost-pump[5]</property>
			<value>0</value>
		</set>
		<set>
			<property>/b707/fuel/valves/boost-pump[6]</property>
			<value>0</value>
		</set>
		<set>
			<property>/b707/fuel/valves/boost-pump[7]</property>
			<value>0</value>
		</set>
		<set>
			<property>/b707/fuel/valves/boost-pump[8]</property>
			<value>0</value>
		</set>
		<set>
			<property>/b707/fuel/valves/boost-pump[9]</property>
