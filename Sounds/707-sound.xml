<!-- engines on work -->

	<engine>
		<name>leftengine</name>
		<path>Sounds/jet.wav</path>
		<mode>looped</mode>
		<condition>	
			<not>
				<property>/sim/current-view/internal</property>
			</not>
			<or>
				<and>
					<property>/engines/engine[0]/running</property>
					<greater-than>
						<property>/engines/engine[0]/n1</property>
						<value>30</value>
					</greater-than>
				</and>
				<and>
					<property>/engines/engine[1]/running</property>
					<greater-than>
						<property>/engines/engine[1]/n1</property>
						<value>30</value>
					</greater-than>
				</and>     
			</or>
		</condition>
		<volume>
			<property>/engines/engine[0]/n1</property>
			<factor>0.015</factor>
		</volume>
		<volume>
			<property>/engines/engine[1]/n1</property>
			<factor>0.015</factor>
		</volume>
		<position>
			<x>4.5</x>
			<y>4.7</y>
			<z>1.2</z>
		</position>
	</engine>

