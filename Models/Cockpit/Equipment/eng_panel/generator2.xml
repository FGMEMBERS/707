<?xml version="1.0"?>
<PropertyList>
 <path>generator.ac</path>

	<animation>
		<type>material</type>
		<object-name>LampAmber</object-name>
		<object-name>LampAmber.001</object-name>
		<object-name>LampAmber.002</object-name>
		<object-name>sockle.lamp</object-name>
		<object-name>sockle</object-name>
		<object-name>sockle.001</object-name>
		<object-name>sockle.002</object-name>  
		<object-name>switch</object-name>  
		<object-name>switch.001</object-name>  
		<object-name>switch.002</object-name>
		<object-name>cover</object-name>
		<object-name>cover.001</object-name>
		<object-name>bezel</object-name>
		<object-name>freqKnob</object-name>
		<condition>
			<not><property>/sim/rendering/rembrandt/enabled</property></not>
		</condition>
		<emission>
		  <red>0.2</red>
		  <green>0.2</green>
		  <blue>0.2</blue>
		  <factor-prop>/controls/lighting/cabin-dim</factor-prop>
		</emission>
	</animation>
	
	<animation>
		<type>material</type>
		<object-name>face</object-name>
		<object-name>needle</object-name>
		<condition>
			<less-than><property>/controls/lighting/cabin-dim</property><value>0.8</value></less-than>
		</condition>
		<emission>
			<red>0.6</red>
			<green>0.4</green>
			<blue>0.3</blue>
			<factor-prop>/controls/lighting/panel-norm</factor-prop>
		</emission>
	</animation>
	
	<animation>
		<type>material</type>
		<object-name>face</object-name>
		<object-name>needle</object-name>
		<condition>
			<not><property>/sim/rendering/rembrandt/enabled</property></not>
			<property>/controls/lighting/cabin-dim</property>
		</condition>
		<emission>
		  <red>0.8</red>
		  <green>0.8</green>
		  <blue>0.8</blue>
		  <factor-prop>/controls/lighting/cabin-dim</factor-prop>
		</emission>
	</animation>
	
	<animation>
		<type>noshadow</type>
		<object-name>LampAmber</object-name>		
		<object-name>LampAmber.001</object-name>
		<object-name>LampAmber.002</object-name>
		<object-name>LampAmberON</object-name>
		<object-name>LampAmberON.001</object-name>
		<object-name>LampAmberON.002</object-name>
	</animation>

	<animation>
		<type>material</type>
		<object-name>LampAmberON</object-name>
		<object-name>LampAmberON.001</object-name>
		<object-name>LampAmberON.002</object-name>
		<emission>
		  <red>   1.0 </red>
		  <green> 0.8 </green>
		  <blue>  0.4 </blue>
		</emission>
	</animation>


 
</PropertyList>
