  <!-- cabin dim --> 
	<animation>
		<type>rotate</type>
		<object-name>turn</object-name>
		<property>/controls/lighting/cabin-dim</property>
		<factor>-150</factor>
		<axis>
		 <x>1</x>
		 <y>0</y>
		 <z>0</z>
		</axis>
		<center>
		<x-m> 0.0</x-m>
		<y-m>-0.236</y-m>
		<z-m>-0.228</z-m>
		</center>  
	</animation>
     
	<animation>
		<type>pick</type>
		<object-name>turn</object-name>
		<action>
		  <button>0</button>
		  <repeatable>true</repeatable>
		  <binding>
		    <command>property-adjust</command>
		    <property>/controls/lighting/cabin-dim</property>
		    <step>0.1</step>
		    <min>0</min>
		    <max>1</max>
		    <wrap>0</wrap>
		  </binding>
		  <binding>
		    <command>property-toggle</command>
		    <property>/sim/sound/switch3</property>
		  </binding>
		</action>
		<action>
		  <button>1</button>
		  <repeatable>true</repeatable>
