<?xml version="1.0"?>
<PropertyList>

  <path>light_switches.ac</path>

	<animation>
		<type>material</type>
		<object-name>sup</object-name>	
		<object-name>sup.001</object-name>	
		<object-name>sup.002</object-name>	
		<object-name>sup.003</object-name>	
		<object-name>switch</object-name>		
		<object-name>switch.001</object-name>		
		<object-name>switch.002</object-name>		
		<object-name>switch.003</object-name>		
		<object-name>switch.004</object-name>		
		<object-name>switch.005</object-name>		
		<object-name>switch.006</object-name>		
		<object-name>switch.007</object-name>	
		<object-name>sockle</object-name>		
		<object-name>sockle.001</object-name>		
		<object-name>sockle.002</object-name>		
		<object-name>sockle.003</object-name>		
		<object-name>sockle.004</object-name>		
		<object-name>sockle.005</object-name>		
		<object-name>sockle.006</object-name>		
		<object-name>sockle.007</object-name>	
		<object-name>turn</object-name>
		<object-name>turn.001</object-name>
		<object-name>turn.002</object-name>
		<condition>
			<less-than><property>/systems/electrical/outputs/cabin-dim</property><value>0.8</value></less-than>
		</condition>
		<emission>
			<red>0.2</red>
			<green>0.2</green>
			<blue>0.2</blue>
			<factor-prop>/systems/electrical/outputs/panel-dim</factor-prop>
		</emission>
	</animation>
	
	<animation>
		<type>material</type>
		<object-name>sup</object-name>	
		<object-name>sup.001</object-name>	
		<object-name>sup.002</object-name>	
		<object-name>sup.003</object-name>		
		<object-name>switch</object-name>		
		<object-name>switch.001</object-name>		
		<object-name>switch.002</object-name>		
		<object-name>switch.003</object-name>		
		<object-name>switch.004</object-name>		
		<object-name>switch.005</object-name>		
		<object-name>switch.006</object-name>		
		<object-name>switch.007</object-name>	
		<object-name>sockle</object-name>		
		<object-name>sockle.001</object-name>		
		<object-name>sockle.002</object-name>		
		<object-name>sockle.003</object-name>		
		<object-name>sockle.004</object-name>		
		<object-name>sockle.005</object-name>		
		<object-name>sockle.006</object-name>		
		<object-name>sockle.007</object-name>	
		<object-name>turn</object-name>
		<object-name>turn.001</object-name>
		<object-name>turn.002</object-name>
		<condition>
			<not><property>/sim/rendering/rembrandt/enabled</property></not>
			<property>/systems/electrical/outputs/cabin-dim</property>
		</condition>
		<emission>
		  <red>0.3</red>
		  <green>0.3</green>
		  <blue>0.3</blue>
		  <factor-prop>/systems/electrical/outputs/cabin-dim</factor-prop>
		</emission>
	</animation>
	
	<!-- Nose-Light -->
  <animation>
      <type>rotate</type>
      <object-name>switch</object-name>
      <object-name>switch.001</object-name>
      <property>/controls/lighting/landing-light</property>
      <factor>50</factor>
      <axis>
       <x>0</x>
       <y>1</y>
       <z>0</z>
      </axis>
      <center>
      <x-m> 0.0325</x-m>
      <y-m> 0.0</y-m>
      <z-m>-0.275</z-m>
      </center>  
  </animation>

  <animation>
      <type>pick</type>
      <object-name>switch</object-name>
      <object-name>switch.001</object-name>
      <object-name>sockle</object-name>	
      <object-name>sockle.001</object-name>		
      <action>
          <button>0</button>
          <repeatable>false</repeatable>
          <binding>
            <command>property-toggle</command>
            <property>/controls/lighting/landing-light</property>
          </binding>		
          <binding>
            <command>property-toggle</command>
            <property>/sim/sound/switch1</property>
          </binding>		
      </action>
  </animation>
  
