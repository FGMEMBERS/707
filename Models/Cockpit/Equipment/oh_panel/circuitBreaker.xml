<PropertyList>
 	<path>circuitBreaker.ac</path>
 	
	<animation>
		<type>material</type>
		<object-name>breakerPanel</object-name>
		<condition>	
			<not><property>/sim/rendering/rembrandt/enabled</property></not>
		</condition>
		<emission>
		  <red>0.3</red>
		  <green>0.3</green>
		  <blue>0.3</blue>
		  <factor-prop>/systems/electrical/outputs/overhead-dim</factor-prop>
		</emission>
	</animation>
	
</PropertyList>
