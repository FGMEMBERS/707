<?xml version="1.0"?>
<PropertyList>

  <path>pilots.ac</path>

	<animation>
		<type>material</type>
		<object-name>pilots</object-name>
		<condition>
			<not><property>/sim/rendering/rembrandt/enabled</property></not>
			<property>/controls/lighting/cabin-dim</property>
		</condition>
		<emission>
		  <red>0.4</red>
		  <green>0.4</green>
		  <blue>0.4</blue>
		  <factor-prop>/controls/lighting/cabin-dim</factor-prop>
		</emission>
	</animation>	

</PropertyList>
