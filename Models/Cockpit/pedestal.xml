	<!-- Throttle lever -->
	<animation>
		<type>rotate</type>
		<object-name>throttle.001</object-name>
		<object-name>reverser.001</object-name>
		<property>/controls/engines/engine[0]/throttle</property>
		<factor>-50</factor>
		<center>
		  <x-m>-18.64</x-m>
		  <y-m>  0.0</y-m>
		  <z-m>  0.65</z-m>
		</center>
		<axis>
		  <x>0.0</x>
		  <y>1.0</y>
		  <z>0.0</z>
		</axis>
	</animation>

	<animation>
		<type>pick</type>
		<object-name>throttle.001</object-name>
		<action>
		  <button>3</button>
		  <repeatable>false</repeatable>
		  <binding>
		    <command>property-adjust</command>
		    <property>/controls/engines/engine[0]/throttle</property>
		    <step>0.05</step>
		    <min>0</min>
		    <max>1</max>
		    <wrap>false</wrap>
		  </binding>
		</action>
		<action>
