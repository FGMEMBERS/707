<?xml version="1.0"?>
<PropertyList>
 <path>cockpit.ac</path>

	<model>
		<name>panel</name>
		<path>Aircraft/707/Models/Cockpit/panel.xml</path>
		<offsets>
		  <x-m>0.0</x-m>
		  <y-m>0.0</y-m>
		  <z-m>0.0</z-m>
		</offsets>
	</model>

	<animation>
		<type>rotate</type>
		<object-name>AileronTab</object-name>
		<property>/controls/flight/aileron-trim</property>
		<factor>-4680</factor>
		<center>
		  <x-m>0.0</x-m>
		  <y-m>-0.055</y-m>
		  <z-m>0.666</z-m>
		</center>
		<axis>
		  <x>1.0</x>
		  <y>0.0</y>
		  <z>0.0</z>
		</axis>
	</animation>
	<animation>
		<type>pick</type>
		<object-name>AileronTab</object-name>
		<action>
		  <button>0</button>
		  <repeatable>true</repeatable>
		  <interval-sec>0.2</interval-sec>
		   <binding>
		    <command>nasal</command>
		    <script>b707.applyTrimWheels(0, 2)</script>
		   </binding>
		</action>
		<action>
		  <button>3</button>
		  <repeatable>true</repeatable>
		  <interval-sec>0.2</interval-sec>
		  <binding>
		    <command>property-adjust</command>
		    <property>/controls/flight/aileron-trim</property>
		    <step>0.01</step>
		    <min>-1</min>
		    <max>1</max>
		    <wrap>false</wrap>
		  </binding>
		</action>
		<action>
		  <button>4</button>
		  <repeatable>true</repeatable>
		  <interval-sec>0.2</interval-sec>
		  <binding>
		    <command>property-adjust</command>
		    <property>/controls/flight/aileron-trim</property>
		    <step>-0.01</step>
		    <min>-1</min>
		    <max>1</max>
		    <wrap>false</wrap>
		  </binding>
		</action>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>ElevatorTab</object-name>
		<property>/controls/flight/elevator-trim</property>
		<factor>-1080</factor>
		<center>
		  <x-m>-18.725</x-m>
		  <y-m>0.0</y-m>
		  <z-m>0.6507</z-m>
		</center>
		<axis>
		  <x>0.0</x>
		  <y>1.0</y>
		  <z>0.0</z>
		</axis>
	</animation>
	<animation>
		<type>pick</type>
		<object-name>ElevatorTab</object-name>
		<action>
		  <button>0</button>
		  <repeatable>true</repeatable>
		  <interval-sec>0.2</interval-sec>
		   <binding>
		    <command>nasal</command>
		    <script>b707.applyTrimWheels(0, 0)</script>
		   </binding>
		</action>
		<action>
		  <button>3</button>
		  <repeatable>true</repeatable>
		  <interval-sec>0.2</interval-sec>
		  <binding>
		    <command>property-adjust</command>
		    <property>/controls/flight/elevator-trim</property>
		    <step>0.01</step>
		    <min>-1</min>
		    <max>1</max>
		    <wrap>false</wrap>
		  </binding>
		</action>
		<action>
		  <button>4</button>
		  <repeatable>true</repeatable>
		  <interval-sec>0.2</interval-sec>
		  <binding>
		    <command>property-adjust</command>
		    <property>/controls/flight/elevator-trim</property>
		    <step>-0.01</step>
		    <min>-1</min>
		    <max>1</max>
		    <wrap>false</wrap>
		  </binding>
		</action>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>RudderTab</object-name>
		<property>/controls/flight/rudder-trim</property>
		<factor>-4680</factor>
		<center>
		  <x-m>-18.743</x-m>
		  <y-m>-0.055</y-m>
		  <z-m> 0.0</z-m>
		</center>
		<axis>
		  <x>0.0</x>
		  <y>0.0</y>
		  <z>1.0</z>
		</axis>
	</animation>
	<animation>
		<type>pick</type>
		<object-name>RudderTab</object-name>
		<action>
		  <button>0</button>
		  <repeatable>true</repeatable>
		  <interval-sec>0.2</interval-sec>
		   <binding>
		    <command>nasal</command>
		    <script>b707.applyTrimWheels(0, 1)</script>
		   </binding>
		</action>
		<action>
		  <button>3</button>
		  <repeatable>true</repeatable>
		  <interval-sec>0.2</interval-sec>
		  <binding>
		    <command>property-adjust</command>
		    <property>/controls/flight/rudder-trim</property>
		    <step>0.01</step>
		    <min>-1</min>
		    <max>1</max>
		    <wrap>false</wrap>
		  </binding>
		</action>
		<action>
		  <button>4</button>
		  <repeatable>true</repeatable>
		  <interval-sec>0.2</interval-sec>
		  <binding>
		    <command>property-adjust</command>
		    <property>/controls/flight/rudder-trim</property>
		    <step>-0.01</step>
		    <min>-1</min>
		    <max>1</max>
		    <wrap>false</wrap>
		  </binding>
		</action>
	</animation>
	
	<!-- Window animations -->
	<!-- Pilot window -->
	
 <animation>
		<name>windowPiComplete</name>
		<object-name>window.pilot</object-name>
		<object-name>window.piop</object-name>
 </animation>
 <!-- turn the knob -->
	<animation>
	<type>rotate</type>
		<object-name>window.piop</object-name>
		<property>sim/multiplay/generic/float[4]</property>
		<interpolation>
			<entry><ind>0.0</ind><dep> 0.0</dep></entry>
			<entry><ind>0.1</ind><dep>-45</dep></entry>
			<entry><ind>1.0</ind><dep>-45</dep></entry>
		</interpolation>
    <axis>   
      <x1-m> -18.812</x1-m>
      <y1-m>  -0.751</y1-m>
      <z1-m>   0.955</z1-m>
      <x2-m> -18.805</x2-m>
      <y2-m>  -0.745</y2-m>
      <z2-m>   0.973</z2-m>
    </axis>
	</animation>
	<!-- go inside -->
	<animation>
	<type>rotate</type>
		<object-name>windowPiComplete</object-name>
		<property>sim/multiplay/generic/float[4]</property>
		<interpolation>
			<entry><ind>0.0</ind><dep> 0.0</dep></entry>
			<entry><ind>0.1</ind><dep> 0.0</dep></entry>
			<entry><ind>0.2</ind><dep> 4</dep></entry>
			<entry><ind>1.0</ind><dep> 4</dep></entry>
		</interpolation>
		<axis>
      <x1-m> -18.812</x1-m>
      <y1-m>  -0.751</y1-m>
      <z1-m>   0.955</z1-m>
      <x2-m> -18.805</x2-m>
      <y2-m>  -0.745</y2-m>
      <z2-m>   0.973</z2-m>
		</axis>
	</animation>
	<!-- slipping back 	-->
	<animation>
	<type>translate</type>
		<object-name>windowPiComplete</object-name>
		<property>sim/multiplay/generic/float[4]</property>
		<interpolation>
			<entry><ind>0.0</ind><dep> 0.0</dep></entry>
			<entry><ind>0.2</ind><dep> 0.0</dep></entry>
			<entry><ind>1.0</ind><dep>-0.5</dep></entry>
		</interpolation>
    <axis>   
      <x1-m> -17.8</x1-m>
      <y1-m>  -1.23</y1-m>
      <z1-m>   0.70</z1-m>
      <x2-m> -18.805</x2-m>
      <y2-m>  -0.80</y2-m>
      <z2-m>   0.973</z2-m>
    </axis>
	</animation>

	<animation>
	<type>rotate</type>
		<object-name>windowPiComplete</object-name>
		<property>sim/multiplay/generic/float[4]</property>
		<interpolation>
			<entry><ind>0.0</ind><dep> 0.0</dep></entry>
			<entry><ind>0.1</ind><dep> 0.0</dep></entry>
			<entry><ind>0.2</ind><dep> 0.01</dep></entry>
			<entry><ind>1.0</ind><dep>  -8</dep></entry>
		</interpolation>
		<axis>
      <x1-m> -17.8</x1-m>
      <y1-m>  -1.23</y1-m>
      <z1-m>   0.70</z1-m>
      <x2-m> -18.805</x2-m>
      <y2-m>  -0.78</y2-m>
      <z2-m>   0.973</z2-m>
		</axis>
	</animation>

  <animation>
		<type>pick</type>
		<object-name>window.piop</object-name>
		<action>
		  <button>0</button>
		  <repeatable>true</repeatable>
		  <interval-sec>0.2</interval-sec>
		  <binding>
		   <command>nasal</command>
		   <script>b707.doorsystem.pilotwinexport();</script>
		  </binding>
		</action>
  </animation>
  
	<!-- Copilot window -->
 <animation>
		<name>windowCoComplete</name>
		<object-name>window.copilot</object-name>
		<object-name>window.coop</object-name>
 </animation>
 <!-- turn the knob -->
	<animation>
	<type>rotate</type>
		<object-name>window.coop</object-name>
		<property>sim/multiplay/generic/float[5]</property>
		<interpolation>
			<entry><ind>0.0</ind><dep> 0.0</dep></entry>
			<entry><ind>0.1</ind><dep> 45</dep></entry>
			<entry><ind>1.0</ind><dep> 45</dep></entry>
		</interpolation>
    <axis>   
      <x1-m> -18.812</x1-m>
      <y1-m>   0.751</y1-m>
      <z1-m>   0.955</z1-m>
      <x2-m> -18.805</x2-m>
      <y2-m>   0.745</y2-m>
      <z2-m>   0.973</z2-m>
    </axis>
	</animation>
	<!-- go inside -->
	<animation>
	<type>rotate</type>
		<object-name>windowCoComplete</object-name>
		<property>sim/multiplay/generic/float[5]</property>
		<interpolation>
			<entry><ind>0.0</ind><dep> 0.0</dep></entry>
			<entry><ind>0.1</ind><dep> 0.0</dep></entry>
			<entry><ind>0.2</ind><dep>-4</dep></entry>
			<entry><ind>1.0</ind><dep>-4</dep></entry>
		</interpolation>
		<axis>
      <x1-m> -18.812</x1-m>
      <y1-m>   0.751</y1-m>
      <z1-m>   0.955</z1-m>
      <x2-m> -18.805</x2-m>
      <y2-m>   0.745</y2-m>
      <z2-m>   0.973</z2-m>
		</axis>
	</animation>
	<!-- slipping back 	-->
	<animation>
	<type>translate</type>
		<object-name>windowCoComplete</object-name>
		<property>sim/multiplay/generic/float[5]</property>
		<interpolation>
			<entry><ind>0.0</ind><dep> 0.0</dep></entry>
			<entry><ind>0.2</ind><dep> 0.0</dep></entry>
			<entry><ind>1.0</ind><dep>-0.5</dep></entry>
		</interpolation>
    <axis>   
      <x1-m> -17.8</x1-m>
      <y1-m>   1.23</y1-m>
      <z1-m>   0.70</z1-m>
      <x2-m> -18.805</x2-m>
      <y2-m>   0.80</y2-m>
      <z2-m>   0.973</z2-m>
    </axis>
	</animation>

	<animation>
	<type>rotate</type>
		<object-name>windowCoComplete</object-name>
		<property>sim/multiplay/generic/float[5]</property>
		<interpolation>
			<entry><ind>0.0</ind><dep> 0.0</dep></entry>
			<entry><ind>0.1</ind><dep> 0.0</dep></entry>
			<entry><ind>0.2</ind><dep> 0.01</dep></entry>
			<entry><ind>1.0</ind><dep>   8</dep></entry>
		</interpolation>
		<axis>
      <x1-m> -17.8</x1-m>
      <y1-m>   1.23</y1-m>
      <z1-m>   0.70</z1-m>
      <x2-m> -18.805</x2-m>
      <y2-m>   0.78</y2-m>
      <z2-m>   0.973</z2-m>
		</axis>
	</animation>
	
  <animation>
		<type>pick</type>
		<object-name>window.copilot</object-name>
		<action>
		  <button>0</button>
		  <repeatable>true</repeatable>
		  <interval-sec>0.2</interval-sec>
		  <binding>
		   <command>nasal</command>
		   <script>b707.doorsystem.copilotwinexport();</script>
		  </binding>
		</action>
  </animation>

</PropertyList>
