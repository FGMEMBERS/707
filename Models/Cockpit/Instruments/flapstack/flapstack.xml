<?xml version="1.0"?>

<PropertyList>

<path>flapstack.ac</path>

	<animation>
		<type>material</type>
		<object-name>Flap.needle</object-name>
		<object-name>Flap.face</object-name>
		<object-name>Flap.needle.001</object-name>
		<object-name>Flap.face.001</object-name>
		<object-name>Slats.needle</object-name>
		<object-name>Slats.needle.001</object-name>
		<condition>
			<less-than><property>/controls/lighting/cabin-dim</property><value>0.8</value></less-than>
		</condition>
		<emission>
			<red>0.6</red>
			<green>0.4</green>
			<blue>0.3</blue>
			<factor-prop>/controls/lighting/panel-norm</factor-prop>
		</emission>
	</animation>

	<animation>
		<type>material</type>
		<object-name>Flap.needle</object-name>
		<object-name>Flap.face</object-name>
		<object-name>Flap.needle.001</object-name>
		<object-name>Flap.face.001</object-name>
		<object-name>Slats.needle</object-name>
		<object-name>Slats.needle.001</object-name>
		<condition>
			<not><property>/sim/rendering/rembrandt/enabled</property></not>
			<property>/controls/lighting/cabin-dim</property>
		</condition>
		<emission>
			<red>0.8</red>
			<green>0.8</green>
			<blue>0.8</blue>
			<factor-prop>/controls/lighting/cabin-dim</factor-prop>
		</emission>
	</animation>

	<animation>
		<type>material</type>
		<object-name>stack</object-name>
		<object-name>Gear.lever</object-name>
		<object-name>Flap.bezel</object-name>
		<object-name>Flap.bezel.001</object-name>
		<condition>
			<not><property>/controls/lighting/cabin-dim</property></not>
		</condition>
		<emission>
			<red>0.05</red>
			<green>0.05</green>
			<blue>0.05</blue>
			<factor-prop>/controls/lighting/panel-norm</factor-prop>
		</emission>
	</animation>
	
	<animation>
		<type>material</type>
		<object-name>stack</object-name>
		<object-name>Gear.lever</object-name>
		<object-name>Flap.bezel</object-name>
		<object-name>Flap.bezel.001</object-name>
		<condition>
			<not><property>/sim/rendering/rembrandt/enabled</property></not>
			<property>/controls/lighting/cabin-dim</property>
		</condition>
		<emission>
		  <red>0.3</red>
		  <green>0.3</green>
		  <blue>0.3</blue>
		  <factor-prop>/controls/lighting/cabin-dim</factor-prop>
		</emission>
	</animation>	

<!-- gears up and down -->
	<animation>
		<type>rotate</type>
		<object-name>Gear.lever</object-name>
		<property>gear/gear[0]/position-norm</property>
		<interpolation>
			<entry><ind>1</ind><dep>-24.5</dep></entry>
			<entry><ind>0.5</ind><dep>0</dep></entry>
			<entry><ind>0</ind><dep> 26</dep></entry> 
		</interpolation>  
		<center>
			<x-m>-0.110</x-m>
			<y-m>0</y-m>
			<z-m>-0.0</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>-1</y>
			<z>0</z>
		</axis>
	</animation>

  <animation>
    <type>pick</type>
    <object-name>Gear.lever</object-name>
    <action>
      <button>0</button>
      <binding>
        <command>property-toggle</command>
        <property>/controls/gear/gear-down</property>
      </binding>
    </action>
  </animation>
  
<!-- the flap instruments -->

<animation>
  <type>rotate</type>
  <object-name>Flap.needle</object-name>
  <property>surface-positions/flap-pos-norm</property>
  <interpolation>
    <entry><ind>   0</ind><dep>  0</dep></entry>
    <entry><ind> 0.2</ind><dep> 72</dep></entry>
    <entry><ind> 0.4</ind><dep>108</dep></entry>
    <entry><ind> 0.6</ind><dep>144</dep></entry>
    <entry><ind>   1</ind><dep>180</dep></entry>
  </interpolation>
	<center>
		<x-m>0.0</x-m>
		<y-m>0.1021</y-m>
		<z-m>-0.0262</z-m>
	</center>
  <axis>
    <x>-1</x>
    <y>0</y>
    <z>0</z>
  </axis>
</animation>

<animation>
  <type>rotate</type>
  <object-name>Flap.needle.001</object-name>
  <property>surface-positions/flap-pos-norm</property>
  <interpolation>
    <entry><ind>   0</ind><dep>  0</dep></entry>
    <entry><ind> 0.2</ind><dep> 72</dep></entry>
    <entry><ind> 0.4</ind><dep>108</dep></entry>
    <entry><ind> 0.6</ind><dep>144</dep></entry>
    <entry><ind>   1</ind><dep>180</dep></entry>
  </interpolation>
	<center>
		<x-m>0.0</x-m>
		<y-m>0.1021</y-m>
		<z-m>0.0223</z-m>
	</center>
  <axis>
    <x>-1</x>
    <y>0</y>
    <z>0</z>
  </axis>
</animation>

<animation>
  <type>rotate</type>
  <object-name>Slats.needle</object-name>
  <property>surface-positions/flap-pos-norm</property>
  <interpolation>
    <entry><ind>   0</ind><dep>  0</dep></entry>
    <entry><ind> 0.2</ind><dep> 36</dep></entry>
    <entry><ind>   1</ind><dep> 36</dep></entry>
  </interpolation>
	<center>
		<x-m>0.0</x-m>
		<y-m>0.1021</y-m>
		<z-m>-0.0262</z-m>
	</center>
  <axis>
    <x>-1</x>
    <y>0</y>
    <z>0</z>
  </axis>
</animation>

<animation>
  <type>rotate</type>
  <object-name>Slats.needle.001</object-name>
  <property>surface-positions/flap-pos-norm</property>
  <interpolation>
    <entry><ind>   0</ind><dep>  0</dep></entry>
    <entry><ind> 0.2</ind><dep> 36</dep></entry>
    <entry><ind>   1</ind><dep> 36</dep></entry>
  </interpolation>
	<center>
		<x-m>0.0</x-m>
		<y-m>0.1021</y-m>
		<z-m>0.0223</z-m>
	</center>
  <axis>
    <x>-1</x>
    <y>0</y>
    <z>0</z>
  </axis>
</animation>


</PropertyList>
